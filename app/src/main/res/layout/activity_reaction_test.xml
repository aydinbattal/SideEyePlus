                android:textSize="20sp" />

            <Button
                android:id="@+id/button_start"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Start"
                tools:ignore="DuplicateIds" />
        </LinearLayout>
    </LinearLayout>

    <Button
